CREATE COMPUTE MODULE RCDMessageFlow_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE BLOB_MSG CHARACTER;
		SET BLOB_MSG=CAST(ASBITSTREAM(InputRoot.XMLNSC.NAME CCSID InputRoot.Properties.CodedCharSetId ENCODING InputRoot.Properties.Encoding)AS CHARACTER CCSID InputRoot.Properties.CodedCharSetId);
		SET OutputRoot.BLOB.BLOB.NAME=BLOB_MSG;
	END;
END MODULE;
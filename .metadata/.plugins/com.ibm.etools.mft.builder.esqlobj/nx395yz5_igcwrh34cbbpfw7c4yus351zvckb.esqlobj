CREATE COMPUTE MODULE Flow2_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE REF REFERENCE TO InputRoot.DFDL.SHEMA.record;
		WHILE LASTMOVE(REF) DO
			SET OutputRoot.DFDL.SHEMA.record.ID = REF.ID;
			SET OutputRoot.DFDL.SHEMA.record.NAME = REF.NAME;
			SET OutputRoot.DFDL.SHEMA.record.ADD = REF.ADD;
			MOVE REF NEXTSIBLING;
		END WHILE;
		--SET OutputRoot.DFDL.SHEMA.record.ID = InputRoot;
	END;
END MODULE;
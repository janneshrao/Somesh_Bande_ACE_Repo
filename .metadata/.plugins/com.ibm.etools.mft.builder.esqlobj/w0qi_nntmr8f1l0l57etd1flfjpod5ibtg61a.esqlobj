/*EATE FUNCTION Main() RETURNS BOOLE*/
	BEGIN
		DECLARE rc BOOLEAN;
		DECLARE getpayload CHARACTER;
		SET getpayload=CAST(ASBITSTREAM(InputRoot.XMLNSC CCSID InputRoot.Properties.CodedCharSetId ENCODING InputRoot.Properties.Encoding)AS CHARACTER CCSID InputRoot.Properties.CodedCharSetId);
		DECLARE DATE_TIME TIMESTAMP CURRENT_TIMESTAMP;
		DECLARE appName CHARACTER ApplicationLabel;
		CALL LOGS(ApplicationLabel,DATE_TIME,getpayload) into rc;
	END;
